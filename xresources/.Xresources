! URxvt Appearance
URxvt.saveLines:        10000
URxvt.xftAntialias:     true
URxvt.font:             xft:Source Code Pro Semibold:style=Semibold:pixelsize=16:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=14:antialias=true,hinting=true
URxvt.boldFont:         xft:Source Code Pro:style=Bold:pixelsize=16:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=16:antialias=true,hinting=true
URxvt.italicFont:       xft:Source Code Pro:style=Italic:pixelsize=14:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=16:antialias=true,hinting=true
URxvt.bolditalicFont:   xft:Source Code Pro:style=Bold Italic:pixelsize=14:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=16:antialias=true,hinting=true
URxvt.letterSpace: -2.5
URxvt.lineSpace: 0
URxvt.internalBorder: 0
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 24
URxvt.geometry: 100x40
URxvt.urlLauncher: firefox
! Fade inactive window
! URxvt.fading: 30
! Transparency (0-1):
! *.alpha: 0.8

Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

